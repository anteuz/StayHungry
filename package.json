  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/dist/",
      "<rootDir>/src/test.ts"
    ],
    "transform": {
      "^.+\\.(ts|mjs|js|html)$": [
        "jest-preset-angular",
        {
          "tsconfig": "<rootDir>/src/tsconfig.spec.json",
          "stringifyContentPathRegex": "\\.(html|svg)$"
        }
      ]
    },
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/app/**/*.ts",
      "!src/app/**/*.spec.ts",
      "!src/app/**/*.module.ts",
      "!src/app/**/*.routing.ts",
      "!src/main.ts",
      "!src/polyfills.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
